
public class PrimeNumbers {
	
	static boolean  isprime(int num)
	{
		if(num<1)
		{
			return false;
		}
		
		for(int i=2;i<num;i++)
		{
			if(num%i==0)
			{
				return false;
			}
		}
		return true;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		boolean res=isprime(15);
		System.out.println("The number " + 15 + "is " + res);
		
		

	}

}
