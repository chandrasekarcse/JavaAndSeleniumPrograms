
public class LargestNo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]= {5,8,96,52,11};
		int max=a[0];
		int less=a[1];
		int seclarg=a[0];
		
		for(int k:a)
		{
			if(max<k)
			{
				max=k;
			}
			if(less>k)
			{
				less=k;
			}
			
		}
		
		for(int j:a)
		{
			if(max>seclarg)
			{
				if(seclarg>less) {
					
					seclarg=j;
					
				}
				
			}
					
					
			
		}
		
		System.out.println(max);
		System.out.println(less);
		System.out.println(seclarg);

	}

}
