public class PracticeExcel {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub

		
	String s="aabbcczzddg";
	char[] a=s.toCharArray();
	int i=0;
	int count;
	int index=0;
	while(i<s.length())
	{
		char b=s.charAt(i);
		count=0;
		for(char c:a)
		{
			if(b==c)
			{
				count++;
				index++;
				
			}
		}
		i=index;
		System.out.print(b+""+count);
	}
	
	}

}
